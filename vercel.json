{
    "version": 2,
    "env": {
        "PRISMA_CLIENT_ENGINE_TYPE": "binary"
    },
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["./prisma/**", "./dist/**"]
            }
        }
    ],
    "ignoreBuilds": ["**/node_modules/@prisma/client/**/*"]
}